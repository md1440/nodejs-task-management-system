{
	"compilerOptions": {
		"module": "commonjs",
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"sourceMap": true,
		"strict": true,
		"skipLibCheck": true,
		"moduleResolution": "node",
		"target": "es2016",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"outDir": "build/"
	},
	"include": ["src","config"],
  "exclude": ["node_modules"]
}
